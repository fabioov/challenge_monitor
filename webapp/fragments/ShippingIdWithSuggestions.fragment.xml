<core:FragmentDefinition
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns="sap.ui.comp.valuehelpdialog"
    xmlns:m="sap.m"
    xmlns:core="sap.ui.core"
>
    <ValueHelpDialog
        id="_IDGenValueHelpDialog"
        title="Shipping Request ID"
        ok=".SearchHelp.onValueHelpWithSuggestionsOkPress"
        cancel=".SearchHelp.onValueHelpWithSuggestionsCancelPress"
        afterClose=".SearchHelp.onValueHelpWithSuggestionsAfterClose"
        supportRanges="true"
        key="ShippingRequestId"
        descriptionKey="ShippingRequestId"
    >
        <filterBar>
            <fb:FilterBar
                id="_IDGenFilterBar1"
                advancedMode="true"
                search=".SearchHelp.onFilterBarWithSuggestionsSearch"
                isRunningInValueHelpDialog="true"               
            >
                <fb:filterGroupItems>
                    <fb:FilterGroupItem
                        id="_IDGenFilterGroupItem"
                        groupName="__$INTERNAL$"
                        name="ShippingRequestId"
                        label="Shipping Request ID"
                        visibleInFilterBar="true"
                    >
                        <fb:control>
                            <m:Input
                                id="_IDGenInput"
                                name="ShippingRequestId"
                                showSuggestion="true"
                                showValueHelp="false"
                                suggestionItems="{
                                    path: '/ZRFSFBP_SH_SHIPPING_ID',
                                    sorter: {
                                        path: 'ShippingRequestId'
                                    }
                                }"
                            >
                                <core:Item
                                    id="_IDGenItem"
                                    key="{ShippingRequestId}"
                                    text="{ShippingrequestId}"
                                />
                            </m:Input>
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem
                        id="_IDGenFilterGroupItem1"
                        groupName="__$INTERNAL$"
                        name="Status"
                        label="Status"
                        visibleInFilterBar="true"
                    >
                        <fb:control>
                            <m:Input
                                id="_IDGenInput1"
                                name="Status"
                                showSuggestion="true"
                                showValueHelp="false"
                                suggestionItems="{
                                    path: '/ZRFSFBP_SH_SHIPPING_ID',
                                    sorter: {
                                        path: 'Status'   
                                    }
                                }"
                            >
                                <core:Item
                                    id="_IDGenItem1"
                                    key="{Status}"
                                    text="{Status}"
                                />
                            </m:Input>
                        </fb:control>
                    </fb:FilterGroupItem>
                    
                </fb:filterGroupItems>
            </fb:FilterBar>
        </filterBar>
    </ValueHelpDialog>
</core:FragmentDefinition>